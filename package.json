{
  "name": "@com.rakuten.esd/elm-live",
  "version": "4.0.3",
  "description": "A flexible dev server for Elm. Live reload included!",
  "bin": {
    "elm-live": "bin/elm-live.js"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "chalk": "^1.1.1",
    "chokidar": "3.0.0",
    "cli-color": "^1.4.0",
    "commander": "2.17.1",
    "cross-spawn": "5.0.1",
    "elm-hot": "^1.0.1",
    "finalhandler": "^1.1.2",
    "internal-ip": "^4.3.0",
    "opn": "^6.0.0",
    "pem": "1.13.2",
    "serve-static": "^1.14.1",
    "url-parse": "^1.4.7",
    "ws": "^7.0.0"
  },
  "scripts": {
    "develop": "ava --watch",
    "test": "nyc ava './lib/test.js' --cwd './lib' && npm run report",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "format": "npm run standard:fix && npm run standard",
    "standard": "standard",
    "standard:fix": "standard --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "engineStrict": true,
  "files": [
    "lib/index.js",
    "lib/src/**/*.js",
    "bin/elm-live.js",
    "License.md"
  ],
  "license": "MIT",
  "keywords": [
    "cli",
    "tool",
    "elm"
  ],
  "author": "William King <contact@wking.io>",
  "repository": {
    "type": "git",
    "url": "git@github.com:wking-io/elm-live"
  },
  "devDependencies": {
    "ava": "1.4.1",
    "coveralls": "3.0.2",
    "dev-null": "^0.1.1",
    "eslint": "5.16.0",
    "eslint-plugin-import": "2.14.0",
    "husky": "0.14.3",
    "inherits": "^2.0.3",
    "nyc": "14.1.1",
    "proxyquire": "^1.7.4",
    "q-stream": "^0.2.0",
    "standard": "12.0.1",
    "strip-ansi": "^3.0.0"
  }
}
